{
    "compilerOptions": {
        "target": "ES2020",
        "moduleResolution": "Node",
        "module": "ES2020",
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./build/",
        "removeComments": true
    }
}